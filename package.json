  "bugs": "https://github.com/BigTeri/jscast/issues",
  "main": "dist/index.js",
  "dependencies": {
    "async": "^2.0.1",
    "commander": "^2.9.0",
    "destroy": "^1.0.4",
    "express": "^4.14.0",
